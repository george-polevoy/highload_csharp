{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog":{
    "MinimumLevel":{
      "Default":"Information",
      "Override":{
        "Microsoft":"Error",
        "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware":"Fatal",
        "System":"Error"
      }
    },
    "WriteTo":[
      {
        "Name":"RollingFile",
        "Args":{
          "pathFormat":"{Date}.log",
          "retainedFileCountLimit":10
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
